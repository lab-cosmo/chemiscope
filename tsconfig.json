{
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "esModuleInterop": true,
    "declaration": true,
    "declarationDir": "dist/",
    "moduleResolution": "Node",
    "sourceMap": true,
    "lib": ["dom", "dom.iterable", "es6"],
    "skipLibCheck": true /* Skip type checking of declaration files */
  },
  "typedocOptions": {
    "entryPoints": "src/index.ts",
    "out": "docs/build/html/api/",
    "excludePrivate": true,
    "hideGenerator": true,
    "readme": "none"
  }
}
